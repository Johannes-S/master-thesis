<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="ERROR">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="[%level]\t\t%msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="Error">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
